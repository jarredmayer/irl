{
  "name": "irl-scraper",
  "version": "1.0.0",
  "description": "Event scraper for IRL Miami/Fort Lauderdale",
  "type": "module",
  "scripts": {
    "scrape": "npx tsx src/index.ts",
    "scrape:verify": "npx tsx src/index.ts --verify-locations",
    "scrape:ai": "npx tsx src/index.ts --ai-editorial",
    "scrape:test": "npx tsx src/index.ts --test",
    "validate": "npx tsx src/validate.ts",
    "verify-locations": "npx tsx src/verify-locations.ts",
    "verify-locations:fix": "npx tsx src/verify-locations.ts --fix"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "cheerio": "^1.0.0",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "node-fetch": "^3.3.2",
    "puppeteer": "^24.36.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "tsx": "^4.15.0",
    "typescript": "^5.4.5"
  }
}
